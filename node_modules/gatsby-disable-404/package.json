{
  "name": "gatsby-disable-404",
  "version": "0.2.1",
  "description": "Disable 404 pages for Gatsby websites",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbstjn/gatsby-disable-404.git"
  },
  "bugs": {
    "url": "https://github.com/sbstjn/gatsby-disable-404/issues"
  },
  "homepage": "https://github.com/sbstjn/gatsby-disable-404",
  "scripts": {
    "test": "jest",
    "test:cover": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "tslint {src,test}/**/*.ts",
    "build": "tsc",
    "prebuild": "rimraf ./gatsby-node.js"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Sebastian MÃ¼ller <mail@sbstjn.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.5",
    "@types/node": "^8.0.19",
    "coveralls": "^2.13.1",
    "dot-json": "^1.0.4",
    "jest": "^20.0.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^20.0.7",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}